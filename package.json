	"keywords": [],
	"scripts": {
		"chain": "NODE_ENV=avalancheFuji hardhat node",
		"clean": "NODE_ENV=avalancheFuji hardhat clean && rm -rf cache",
		"compile": "NODE_ENV=avalancheFuji hardhat compile",
		"coverage": "NODE_ENV=avalancheFuji hardhat coverage",
		"deploy": "NODE_ENV=avalancheFuji hardhat deploy --network localhost",
		"deploy:arbitrumSepolia": "NODE_ENV=arbitrumSepolia hardhat deploy --tags TicTacAvax --network arbitrumSepolia",
		"deploy:baseSepolia": "NODE_ENV=baseSepolia hardhat deploy --tags TicTacAvax --network baseSepolia",
		"deploy:avalancheFuji": "NODE_ENV=avalancheFuji hardhat deploy --tags TicTacAvax --network avalancheFuji",
		"deploy:celoAlfajores": "NODE_ENV=celoAlfajores hardhat deploy --tags TicTacAvax --network celoAlfajores",
		"deploy:cross:arbitrumSepolia": "NODE_ENV=arbitrumSepolia hardhat deploy --tags arbitrumSepolia --network arbitrumSepolia",
		"deploy:cross:avalancheFuji": "NODE_ENV=avalancheFuji hardhat deploy --tags avalancheFuji --network avalancheFuji",
		"deploy:cross:baseSepolia": "NODE_ENV=baseSepolia hardhat deploy --tags baseSepolia --network baseSepolia",
		"deploy:cross:celoAlfajores": "NODE_ENV=celoAlfajores hardhat deploy --tags celoAlfajores --network celoAlfajores",
		"format": "prettier --write \"./**/*.{js,ts}\"",
		"lint": "eslint . --fix --ext \"./**/*.{js,ts,sol}\"",
		"test": "NODE_ENV=avalancheFuji hardhat test"
	},
	"dependencies": {
		"@axelar-network/axelar-cgp-solidity": "^6.3.1",
